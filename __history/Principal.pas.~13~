{$R *.dfm}

procedure TForm1.column21Click(Sender: TObject);
begin
  edDisplay.Text := edDisplay.Text + '7';
end;

procedure TForm1.column22Click(Sender: TObject);
begin
    edDisplay.Text := edDisplay.Text + '8';
